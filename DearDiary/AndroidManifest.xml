<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deardiary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/diary1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.deardiary.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.DearDiary"
            android:label="@string/title_activity_dear_diary"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.DearDiarySignUp"
            android:label="@string/title_activity_dear_diary_sign_up"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.Firstpage"
            android:label="@string/title_activity_firstpage"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.deardiary.Friendinfo"
            android:label="@string/title_activity_friendinfo"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.Friendinfo1"
            android:label="@string/title_activity_friendinfo1"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.FriendDetails"
            android:label="@string/title_activity_friend_details"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.Model"
            android:label="@string/title_activity_model"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.Password_Recovery"
            android:label="@string/title_activity_password__recovery"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.BankInfo"
            android:label="@string/title_activity_bank_info"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.BankInfo1"
            android:label="@string/title_activity_bank_info1"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.deardiary.BankDetails"
            android:label="@string/title_activity_bank_details" >
        </activity>
        <activity
            android:name="com.example.deardiary.UpdateFriends"
            android:label="@string/title_activity_update_friends" >
        </activity>
        <activity
            android:name="com.example.deardiary.Reminder"
            android:label="@string/title_activity_reminder" >
        </activity>
    </application>

</manifest>
